generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Session {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  accountId   String
  sessionId   String
  accessToken String
}
